<?php

namespace Tm\TeambuilderBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * TypeAttaque
 *
 * @ORM\Table(name="type_attaque")
 * @ORM\Entity(repositoryClass="Tm\Teambuilder\Entity\TypeAttaqueRepository")
 */
class TypeAttaque
{
    /**
     * @var integer
     *
     * @ORM\Column(name="ID", type="integer", nullable=false)
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="IDENTITY")
     */
    private $id;

    /**
     * @var string
     *
     * @ORM\Column(name="LIBELLE", type="string", length=64, nullable=false)
     */
    private $libelle;

    /**
     * @var string
     *
     * @ORM\Column(name="CODE", type="string", length=16, nullable=false)
     */
    private $code;

    /**
     * @var \Doctrine\Common\Collections\Collection
     *
     * @ORM\ManyToMany(targetEntity="Champion", mappedBy="idTypeAttaque")
     */
    private $idChampion;

    /**
     * Constructor
     */
    public function __construct()
    {
        $this->idChampion = new \Doctrine\Common\Collections\ArrayCollection();
    }


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set libelle
     *
     * @param string $libelle
     * @return TypeAttaque
     */
    public function setLibelle($libelle)
    {
        $this->libelle = $libelle;

        return $this;
    }

    /**
     * Get libelle
     *
     * @return string 
     */
    public function getLibelle()
    {
        return $this->libelle;
    }

    /**
     * Set code
     *
     * @param string $code
     * @return TypeAttaque
     */
    public function setCode($code)
    {
        $this->code = $code;

        return $this;
    }

    /**
     * Get code
     *
     * @return string 
     */
    public function getCode()
    {
        return $this->code;
    }

    /**
     * Add idChampion
     *
     * @param \Tm\TeambuilderBundle\Entity\Champion $idChampion
     * @return TypeAttaque
     */
    public function addIdChampion(\Tm\TeambuilderBundle\Entity\Champion $idChampion)
    {
        $this->idChampion[] = $idChampion;

        return $this;
    }

    /**
     * Remove idChampion
     *
     * @param \Tm\TeambuilderBundle\Entity\Champion $idChampion
     */
    public function removeIdChampion(\Tm\TeambuilderBundle\Entity\Champion $idChampion)
    {
        $this->idChampion->removeElement($idChampion);
    }

    /**
     * Get idChampion
     *
     * @return \Doctrine\Common\Collections\Collection 
     */
    public function getIdChampion()
    {
        return $this->idChampion;
    }
}
